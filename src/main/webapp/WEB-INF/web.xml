<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

<context-param>
<param-name>cgst</param-name>
<param-value>0.10</param-value>
</context-param>


<servlet>
<init-param>
<param-name>sgstofkar</param-name>
<param-value>0.50</param-value>
</init-param>
<servlet-name>k</servlet-name>
<servlet-class>gsta2.KarServlet</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>k</servlet-name>
<url-pattern>/kar</url-pattern>
</servlet-mapping>

<servlet>
<init-param>
<param-name>sgstofmah</param-name>
<param-value>0.60</param-value>
</init-param>
<servlet-name>m</servlet-name>
<servlet-class>gsta2.MahServlet</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>m</servlet-name>
<url-pattern>/mah</url-pattern>
</servlet-mapping>


</web-app>
